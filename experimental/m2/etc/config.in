# config

[dirs]
auxdir          = @_PACKAGE_DATADIR@/aux
templatesdir    = @_PACKAGE_DATADIR@/templates
m4dir           = %(auxdir)s/m4


[vmm]
connect = qemu:///system
vmxmldir = @_LIBVIRT_DOMAIN_XMLDIR@
vmxmlstoredir = @_DOMAIN_XMLDIR@
vnetxmldir = @_LIBVIRT_NETWORK_XMLDIR@


[pack]
variant         = minimal
version         = 0.1
compress        = xz
target_dist     = fedora-12-i386


[virtinst]
site_config = @_PACKAGE_SYSCONFDIR@/profiles/site.cfg
template = @_PACKAGE_DATADIR@/templates/virtinst/virtinst.tmpl


