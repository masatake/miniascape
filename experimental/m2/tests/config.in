# config

[dirs]
auxdir          = @abs_top_srcdir@/aux
templatesdir    = @abs_top_srcdir@/data/templates
m4dir           = %(auxdir)s/m4


[vmm]
connect = qemu:///system
vmxmldir = @abs_top_srcdir@/tests
vmxmlstoredir = @abs_top_srcdir@/tests
vnetxmldir = @abs_top_srcdir@/tests


[pack]
variant         = minimal
version         = 0.1
compress        = xz
target_dist     = fedora-12-i386


[virtinst]
site_config = @_PACKAGE_SYSCONFDIR@/profiles/site.cfg
template = @_PACKAGE_DATADIR@/templates/virtinst/virtinst.tmpl


