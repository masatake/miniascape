# config

[commands]
qemu_img = @QEMU_IMG@
virt_install = @VIRTINST@
svc_libvirtd = @SERVICE_LIBVIRTD@


[dirs]
auxdir = @@pkgdatadir@@/aux
m4dir = %(auxdir)s/m4


[package]
domain_variant = minimal 
name_prefix = vm
name_suffix = %(domain_variant)s
srczip_ext = xz


[virtinst]
site_config = @@pkgsysconfdir@@/profiles/site.cfg
template = @@pkgdatadir@@/templates/virtinst/virtinst.tmpl


