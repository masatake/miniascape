#
# This is the site global profile data.
#
# @see virt-install(1)
#

virtinst: @VIRTINST@

# hypervisor connection
connect: qemu:///system

# RAM size in MB
ram: 256
arch: i686
vcpus: 1
keymap: en-us

# CPU affinitiy
#cpuset: auto

# Amount of time to wait in minutes for a virtual domain to complete its install. 
wait: 20

