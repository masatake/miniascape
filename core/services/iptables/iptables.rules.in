# HTTP and HTTP/SSL
-A INPUT -i @NET_1_BRIDGE@ -m state --state NEW -p tcp --dport 80 -j ACCEPT
-A INPUT -i @NET_1_BRIDGE@ -m state --state NEW -p tcp --dport 443 -j ACCEPT
# NFSv4
-A INPUT -i @NET_1_BRIDGE@ -m state --state NEW -p tcp --dport 2049 -j ACCEPT
# iSCSI
-A INPUT -i @NET_1_BRIDGE@ -m state --state NEW -p tcp --dport 3260 -j ACCEPT
# avahi
-A INPUT -i @NET_1_BRIDGE@ -p udp --dport 5353 -j ACCEPT
# fence_virtd
-A INPUT -i @NET_1_BRIDGE@ -p udp -m udp --dport 1229 -j ACCEPT
