touch /etc/yum.repos.d/rhel-kstree.repo.save
cat << EOF > /etc/yum.repos.d/rhel-kstree.repo
[rhel-kstree]
name=Red Hat Enterprise Linux $releasever - $basearch - Server (kickstart tree)
baseurl=@AUTOINST_TREE_URL_BASE@/rhel/5/0/server/$basearch/Server/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release
EOF
